{
  "expo": {
    "name": "InmiJobs",
    "slug": "InmiJobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icono.png",
    "scheme": "inmijobs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/img/foreground.png"
      },
      "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#B85CFB",
      "translucent": false
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rbkmeyer.InmiJobs"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cff2f934-9d7b-4654-bce0-ab67d39a7edc"
      }
    }
  }
}
